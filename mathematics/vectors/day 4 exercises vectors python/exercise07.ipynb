   "source": [
    "import numpy as np\n",
    "\n",
    "vetor_linha = np.array([ [1,2,3], [4,5,6] ]) # (2x3) -> (3x2)\n",
    "\n",
    "vetor_coluna = vetor_linha.T  \n",
    "print(f\"Antes:\\n {vetor_linha}\")\n",
    "print(f\"Após:\\n {vetor_coluna}\")\n"
   ]
  }
