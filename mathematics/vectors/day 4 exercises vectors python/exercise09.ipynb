   "source": [
    "# Crie uma vetor 6x1 (linhas) e transforme em um vetor 1x6 (colunas)\n",
    "import numpy as np\n",
    "# Criando o vetor \n",
    "vetor = np.array([[1], [2], [3], [4], [5], [6]])\n",
    "print(f\"Antes:\\n {vetor}\")\n",
    "\n",
    "# Transpondo ele\n",
    "vetor_t = vetor.T\n",
    "print(f\"\\nApos:\\n {vetor_t}\")\n"
   ]
  }
