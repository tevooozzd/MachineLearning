   ],
   "source": [
    "# Criar um vetor de tamanho 12 e separá-lo em 3 vetores de tamanho 4.\n",
    "import numpy as np\n",
    "\n",
    "# Criando o vetor\n",
    "vetor = np.random.randint(1,10,12)\n",
    "print(f\"Antes da reorganização:\\n{vetor}\")\n",
    "\n",
    "# Substituindo\n",
    "vetor = vetor.reshape(3,4)\n",
    "print(f\"\\nApós:\\n{vetor}\")"
   ]
  }
